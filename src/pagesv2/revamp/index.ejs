  <script src="/assts/js/download.js"></script>
  <script src="/assts/js/beforeafter/beforeafter.jquery-1.0.0.js"></script>
  <script src="/assts/js/jquery.validate.js"></script>
  <!-- <script src="/assts/js/dragdrop.js"></script> -->
  <script src="/assts/js/sendwhatsapp.js"></script>
  <script src="https://unpkg.com/aos@2.3.1/dist/aos.js"></script>
  <!-- <script src="/assts/js/upload-people-counting.js"></script> -->
  <script src="/assts/js/upload-compress.js"></script>
  <script src="https://unpkg.com/axios/dist/axios.min.js"></script>
  <script src="/assts/js/navbar.js"></script>

  <!-- custom script -->
  <script>
    $('.trial__tabs .nav-link').click(function () {
      var selectedNavItem = document.querySelector(".nav-link.active");
      var selectedNavItemID = selectedNavItem.getAttribute("id");

      if (selectedNavItemID == 'pills-compress-tab') {
        $('.section__compress').removeClass('d-none')
        $('.section__people').addClass('d-none')
      } else {
        $('.section__compress').addClass('d-none')
        $('.section__people').removeClass('d-none')
      }
    })
  </script>
  <script>
    AOS.init();
  </script>
